<nav class="">
  <% if user_signed_in? %>
    <p>Signed in as <%= current_user.email %><%= link_to "Sign out", destroy_user_session_path, method: :delete %></p>
  <% else %>
    <p>
      <%= link_to "Sign in", new_user_session_path %>
      <%= link_to "Sign up", new_user_registration_path %>
    </p>
  <% end %>
  <ul>
    <li> <%= link_to "Store Home", store_path %> </li>
    <% if current_user && current_user.current_cart %>
      <li> <%= link_to "Cart", cart_path(current_user.current_cart) %></li>
    <% end %>
  </ul>
</nav>
